@model EntityLayer.Concrete.Message2
@{
    ViewData["Title"] = "MessageDetails";
    Layout = "~/Views/Shared/WriterLayout.cshtml";
}

<h1>Message Detail Page</h1>
<br />
    @Html.Label("Message Id")
    @Html.TextBoxFor(x => x.MessageId, new { @class = "form-control" })
    <br />
    @Html.Label("Message Subject")
    @Html.TextBoxFor(x => x.Subject, new { @class = "form-control" })
    <br />
    @Html.Label("Message Content")
    @Html.TextAreaFor(x => x.MessageDetails,10,3, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Go Back To Messages</button>


